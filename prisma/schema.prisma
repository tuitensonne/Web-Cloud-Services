generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  ID       Int    @id @default(autoincrement())
  email    String @unique @db.VarChar(100)
  password String @db.VarChar(100)
  username String @db.VarChar(100)
}
